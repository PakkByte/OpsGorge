N8N_HOST=your-pc-tunnel.example.com
N8N_PROTOCOL=https
WEBHOOK_URL=https://your-pc-tunnel.example.com
N8N_SECURE_COOKIE=true
N8N_SESSION_COOKIE_SAMESITE=lax

DB_TYPE=postgresdb
POSTGRES_DB=n8n
POSTGRES_USER=n8n
POSTGRES_PASSWORD=
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

N8N_PORT=5678
N8N_DIAGNOSTICS_ENABLED=false
N8N_METRICS=false
N8N_LOG_LEVEL=info
N8N_BLOCK_ENV_ACCESS_IN_NODE=false
N8N_GIT_NODE_DISABLE_BARE_REPOS=true
N8N_RUNNERS_ENABLED=true

# Windows: n8n (Docker) ? Ollama (Docker service)
OLLAMA_BASE_URL=http://ollama:11434
